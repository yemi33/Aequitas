{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": "off",
        "semi": "off",
        "quotes": "off",
        "no-unused-vars": "off",
        "no-extra-semi": "off",
        "multiline-ternary": "off",
        "react/prop-types": "off"
    }
}
